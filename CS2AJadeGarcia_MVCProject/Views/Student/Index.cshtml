@model IEnumerable<CS2AJadeGarcia_MVCProject.BusLogic.Model.TblStudent>

<div>
    <h2>Student List</h2>
    <a class="btn btn-primary m-2" href="/Student/AddNewStudent"> Add New Student</a>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
        @foreach(var student in Model)
        {
            <tr> 
                <td>@student.firstname</td>
                <td>@student.lastname</td>
                <td>@student.age</td>
                <td>@student.course</td>
            </tr>
        }
    </table>
</div>